                var delivery_amount = 0;
                var promo_k = 1;
                var discount = '';

                if (typeof (bc.basket_free_delivery) !== 'undefined') {

                    var free_delivery_from = bc.basket_free_delivery_from;
                    free_delivery_from = parseFloat(free_delivery_from);
